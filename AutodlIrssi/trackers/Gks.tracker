<trackerinfo
	type="gks"
	shortName="Gks"
	longName="Gks"
	siteName="gks.gs">

	<settings>
		<cookie_description/>
		<cookie/>
	</settings>

	<servers>
		<server
			network="irc.gks.gs"
			serverNames="irc.gks.gs"
			channelNames="#GKS"
			announcerNames="GKS"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--[DVDRip/BDRip] - https://gks.gs/torrent/17339/Bebe.mode.d'emploi.(Katherine.Heigl,.Josh.Duhamel,.Josh.Lucas).VFSTFR.SeeDeaF [701.90 Mo]-->
				<regex value="^\[(.*)\]\s*-\s*https:\/\/gks.gs\/torrent\/(\d+)\/(.*)\s*\[(.*)\]"/>
				<vars>
					<var name="category"/>
					<var name="$torrentId"/>
					<var name="$baseName"/>
					<var name="$torrentSize"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentName">
				<var name="$baseName"/>
			</var>
			<var name="torrentUrl">
				<string value="https://gks.gs/get/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="$baseName"/>
			</var>
			<http name="cookie">
				<var name="cookie"/>
			</http>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
