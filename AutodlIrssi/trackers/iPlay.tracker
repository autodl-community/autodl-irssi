<trackerinfo
	type="iplay"
	shortName="iPlay"
	longName="iPlay"
	siteName="www.iplay.ro">

	<settings>
		<cookie_description/>
		<cookie/>
		<passkey/>
	</settings>

	<servers>
		<server
			network="iPLAY"
			serverNames="irc.iplay.ro"
			channelNames="#iplay"
			announcerNames="iPlay"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--New in TV/Episodes -> Cops.S24E04.Evading.Arrests.HDTV.XviD-2HD Size: 174.88 MB Link: http://www.iplay.ro/details.php?id=153205  -->
				<regex value="^New in (.*) -> (.*) Size: (.*B) Link:\s*https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)"/>
				<vars>
					<var name="category"/>
					<var name="torrentName"/>
					<var name="$torrentSize"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent?passkey="/>
				<varenc name="passkey"/>
			</var>
			<http name="cookie">
				<var name="cookie"/>
			</http>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
