<trackerinfo
	type="rtt"
	shortName="RevoTT"
	longName="RevolutionTT"
	siteName="revolutiontt.me">

	<settings>
		<description text="Paste (Ctrl+V) any RevolutionTT torrent download link into the text box below to automatically extract passkey."/>
		<passkey/>
	</settings>

	<servers>
		<server
			network="thehorde"
			serverNames="irc.revolutiontt.me"
			channelNames="#vip,#announce"
			announcerNames="RevoTT"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--[NEWmTORRENT]jinfTV/XViDY->HBones.S05E12.The.Proof.in.the.Pudding.HDTV.XviD-FQMA| https://www.revolutiontt.net/details.php?id=pyK9laiE&hit=1-->
				<regex value="\[NEW.TORRENT\].in.(.*).->.(.*).\| (https?://.*/).*id=([0-9a-zA-Z]+)"/>
				<vars>
					<var name="category"/>
					<var name="torrentName"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
			<extract>
				<!--[NEWyREQUESTEDOTORRENT]CindTV/PacksY->v[REQ]BOneTTreeaHillHSeasonJ6z| https://www.revolutiontt.net/details.php?id=pAKvOOfg&hit=1-->
				<regex value="\[NEW.REQUESTED.TORRENT\].in.(.*).->.(.*).\| (https?://.*/).*id=([0-9a-zA-Z]+)"/>
				<vars>
					<var name="category"/>
					<var name="torrentName"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
			<extract>
				<!--!new Elizabeth.Is.Secret.Agents.S01E01.1080p.HDTV.H264-QPEL | TV/HDx264 | https://revolutiontt.me/details.php?id=pA6oPRsg&hit=1-->>
				<regex value="!new (.*) \| (.*) \| (https?://.*/).*id=([0-9a-zA-Z]+)"/>
				<vars>
					<var name="torrentName"/>
					<var name="category"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<var name="$baseUrl"/>
				<string value="download.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent?passkey="/>
				<var name="passkey"/>
			</var>
		</linematched>
		<ignore>
			<regex value="^\[NEW.TORRENT\]" expected="false"/>
		</ignore>
	</parseinfo>
</trackerinfo>
