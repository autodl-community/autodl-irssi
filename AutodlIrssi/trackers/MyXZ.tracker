<trackerinfo
    type="myxz"
    shortName="MyXZ"
    longName="Xtreme Zone"
    siteName="myxz.eu">

    <settings>
        <description text="Copy your passkey from the profile and paste it in the field below."/>
        <passkey/>
    </settings>

    <servers>
        <server
            network="P2P-NET"
            serverNames="irc.p2p-network.net,irc.p2p-irc.net"
            channelNames="#myxz"
            announcerNames="myxz-announcer"
            />
    </servers>

    <parseinfo>
        <linepatterns>
            <extract>
              <!--New! (TV HD) - Swamp.People.Serpent.Invasion.S01E02.720p.WEB.h264-TRUMP - (945.72 MB) - https://myxz.eu/browse/134953/t/swamp-people-serpent-invasion-s01e02-720p-web-h264-trump -->
              <regex value="^New! \((.*)\) - (.*) - \((.*)\) - https?:\/\/(.*\/)browse.*?(\d+).*"/>
              <vars>
                <var name="category"/>
                <var name="torrentName"/>
                <var name="$torrentSize"/>
                <var name="$baseUrl"/>
                <var name="$torrentId"/>
              </vars>
            </extract>
        </linepatterns>
        <linematched>
          <var name="torrentUrl">
            <string value="https://"/>
            <var name="$baseUrl"/>
            <string value="download.php/"/>
            <var name="$torrentId"/>
            <string value="/"/>
            <var name="torrentName"/>
            <string value=".torrent?passkey="/>
            <var name="passkey"/>
          </var>
        </linematched>
        <ignore>
        </ignore>
    </parseinfo>
</trackerinfo>
